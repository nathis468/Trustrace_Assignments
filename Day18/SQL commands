CREATE DATABASE Student_exams;

USE Student_exams;

CREATE TABLE Student(
	StudentId VARCHAR(10),
	FirstName VARCHAR(10),
	LastName VARCHAR(10),
	StudentYear INT(4),
	StudentDpt VARCHAR(10)
);
		
