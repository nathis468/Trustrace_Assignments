CREATE TABLE Courses(
	CourseId VARCHAR(10),
	CourseName VARCHAR(30),
	Semester INT,
	Regulation INT
);

CREATE TABLE Exams(
	ExamName VARCHAR(30),
	ExamDate DATE,
    	Place VARCHAR(30),
    	ExamTime INT
);








CREATE DATABASE Players;

USE Players;

CREATE TABLE Team(
	Players VARCHAR(30),
	MatchDate DATE,
	Scores INT,
	MatchesCount INT,
	Location VARCHAR(30),
	OpponentTeam VARCHAR(30)
);

CREATE TABLE Player(
	PlayerName VARCHAR(30),
	Matches INT,
    	Skills VARCHAR(20)
);
    
    
    
    
    
    
    
    
    
 CREATE DATABASE Student_courses;   
 
 USE Student_courses;
 
 CREATE TABLE Student(
	StudentId VARCHAR(10),
	FirstName VARCHAR(10),
	LastName VARCHAR(10),
	StudentYear INT,
	StudentDpt VARCHAR(10)
);
 
 CREATE TABLE CoursesOffering(
	CourseId VARCHAR(10),
	SectionNo INT,
	Semester INT,
	StudentYear INT,
	Timings TIME,
    Venue VARCHAR(20),
    Instructor VARCHAR(20)
);

CREATE TABLE Instructor(
	InstructorId VARCHAR(10),
	InstructorName VARCHAR(20),
	Title VARCHAR(30),
    Department VARCHAR(10)
);


CREATE TABLE Courses(
	CourseId VARCHAR(10),
	CourseName VARCHAR(30),
	Semester INT,
	Regulation INT,
    	Credits INT,
	Syllabus VARCHAR(100)
);














































 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
